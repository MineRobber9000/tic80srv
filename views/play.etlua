<h1>Play</h1>
<h2>Grab Bag</h2>
<% render("widgets.cart_list", {carts = grab_bag}) %>
<% for i=1,#categories do local category = categories[i] %>
<h2><a href="<%- url_for('play',nil,{sort=category.key}) %>" class="link"><%= category.name %></a></h2>
<% render("widgets.cart_list", {carts = category.sample}) %>
<% end %>